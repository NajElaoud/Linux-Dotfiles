    },
    {
      "type": "terminal",
      "key": "└─[TERMINAL]──>",
      "keyColor": "#00d9ff"
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[38;2;138;43;226m╔══════════════════════════════════════════════════════════╗"
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;138;43;226m║  \u001b[38;2;255;0;255m>> HARDWARE.PROBE.ACTIVE \u001b[38;2;138;43;226m                               ║"
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;138;43;226m╚══════════════════════════════════════════════════════════╝"
