{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty-direct",
    "source": "/home/LoOtGobLiN/.config/fastfetch/logo/john_starrail.png",
    "padding": {
      "top": 1,
      "left": 2
    }
  },
  "display": {
    "separator": " ",
    "bar": {
      "width": 22,
      "char": {
        "elapsed": "█",
        "total": "░"
      },
      "border": {
        "left": "▐",
        "right": "▌"
      },
      "color": {
        "elapsed": "#ff6b9d",  /* Vibrant pink */
        "total": "#2d1b3d",
        "border": "#ffd700"    /* Gold */
      }
    }
  },
  "modules": [
    {
      "type": "custom",
      "format": "\u001b[38;2;255;107;157m▂▄▆█ \u001b[38;2;139;233;253mGAMING STATION\u001b[38;2;255;107;157m █▆▄▂"
    },
    "break",
    {
      "type": "title",
      "keyWidth": 10,
      "keyColor": "#ff6b9d"  /* Hot pink */
    },
    {
      "type": "os",
      "key": "⚡ OS",
      "keyColor": "#ffd700"  /* Gold */
    },
    {
      "type": "kernel",
      "key": "├ Kernel",
      "keyColor": "#ffb700"  /* Amber gold */
    },
    {
      "type": "packages",
      "key": "├󰏖 Packages",
      "keyColor": "#ffa500"  /* Orange */
    },
    {
      "type": "shell",
      "key": "├ Shell",
      "keyColor": "#ff8c00"  /* Dark orange */
    },
    {
      "type": "localIP",
      "key": "├󰩟 Local IP",
      "keyColor": "#ff7f50"  /* Coral */
