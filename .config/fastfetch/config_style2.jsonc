      "keyColor": "#e5d3ff"
    },
    {
      "type": "icons",
      "key": "├─ Icons",
      "keyColor": "#efe3ff"
    },
    {
      "type": "font",
      "key": "├─ Fonts",
      "keyColor": "#f9f3ff"
    },
    {
      "type": "terminal",
      "key": "└─ Terminal",
      "keyColor": "#e8d4ff"
    },
    "break",
    {
      "type": "chassis",
      "key": "♦ PC",
      "keyColor": "#8be9fd"
    },
    {
      "type": "board",
      "key": "├─ Board",
      "keyColor": "#9feafd"
